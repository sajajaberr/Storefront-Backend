{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node ./dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "npm run build && npm run jasmine",
    "tsc": "tsc",
    "build": "tsc",
    "dev": "nodemon src/server.ts",
    "jasmine": "jasmine",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "migrate:reset": "db-migrate reset",
    "migrate:up:test": "dotenv -e .env -- db-migrate up -e test",
    "migrate:down:test": "dotenv -e .env -- db-migrate down -e test"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jasmine": "^3.10.18",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^10.0.0",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^5.9.3"
  }
}
